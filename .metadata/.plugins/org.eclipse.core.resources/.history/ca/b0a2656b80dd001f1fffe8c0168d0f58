
public class VolumeCalculator {

	// Fill the code
	private double radius;
	private double heightCylinder;
	private int length;
	private int breadth;
	private int heightCuboid;
	
	public VolumeCalculator(double radius, double heightCylinder) {
		this.radius = radius;
		this.heightCylinder = heightCylinder;
	}
	
	public VolumeCalculator(int lenght, int breadth, int heightCuboid) {
		this.length = length;
		this.breadth = breadth;
		this.heightCuboid = heightCuboid;
	}
	
	public double calculateVolume(double radius, double heightCylinder) {
		double volumeOfCylinder = 3.14*radius*radius*heightCylinder;
		return volumeOfCylinder;
	}
	
	public double calculateVolume(int length, int breadth, int height) {
	    double volumeOfCuboid = length*breadth*height;
	    return volumeOfCuboid;
	}
	
}
